ef=\"https:\/\/github.com\/Prerna1405\" target=\"_blank\">\/prernaahire<\/a><\/p>\r\n    <\/div>\r\n\r\n    <form class=\"contact-form\">\r\n      <input type=\"text\" placeholder=\"Your Name\" required>\r\n      <input type=\"email\" placeholder=\"Your Email\" required>\r\n      <textarea placeholder=\"Your Message\" required><\/textarea>\r\n      <button type=\"submit\">Send Message<\/button>\r\n    <\/form>\r\n  <\/div>\r\n<\/section>\r\n","_padding":{"unit":"px","top":"0","right":"0","bottom":"0","left":"280","isLinked":false},"custom_css":"\/* CSS *\/\r\n.contact-container h2,\r\n.contact-container p,\r\n.contact-form input,\r\n.contact-form textarea,\r\n.contact-form button {\r\n  font-family: 'Preahvihear', sans-serif;\r\n}\r\n\r\nbody {\r\n  margin: 0;\r\n  background-color: #0d0d0d;\r\n  font-family: 'Poppins', sans-serif;\r\n  color: #eee;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  min-height: 100vh;\r\n}\r\n\r\n\/* Section Styling *\/\r\n.contact-section {\r\n  width: 100%;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  padding: 60px 20px;\r\n  background: linear-gradient(135deg, #0d0d0d 0%, #1e1e1e 100%);\r\n  animation: fadeIn 1.5s ease-out forwards;\r\n}\r\n\r\n\/* Container *\/\r\n.contact-container {\r\n  background-color: #1a1a1a;\r\n  padding: 40px;\r\n  max-width: 600px;\r\n  width: 100%;\r\n  border-radius: 20px;\r\n  box-shadow: 0 20px 60px rgba(178, 102, 255, 0.2);\r\n  transform: scale(0.95);\r\n  animation: slideUp 1.2s ease forwards;\r\n}\r\n\r\n\/* Heading *\/\r\n.contact-container h2 {\r\n  font-size: 2.5rem;\r\n  color: #b266ff;\r\n  margin-bottom: 10px;\r\n  text-align: center;\r\n}\r\n\r\n\/* Text *\/\r\n.contact-container p {\r\n  color: #ccc;\r\n  text-align: center;\r\n  margin-bottom: 25px;\r\n  line-height: 1.6;\r\n}\r\n\r\n\/* Contact Info *\/\r\n.contact-info {\r\n  margin-bottom: 25px;\r\n  text-align: center;\r\n}\r\n\r\n.contact-info a {\r\n  color: #b266ff;\r\n  text-decoration: none;\r\n  transition: 0.3s;\r\n}\r\n\r\n.contact-info a:hover {\r\n  color: #fff;\r\n}\r\n\r\n\/* Form Inputs *\/\r\n.contact-form input,\r\n.contact-form textarea {\r\n  width: 100%;\r\n  padding: 15px;\r\n  margin-bottom: 20px;\r\n  border: none;\r\n  background: #2a2a2a;\r\n  border-radius: 10px;\r\n  color: #fff;\r\n  font-size: 1rem;\r\n  transition: 0.3s;\r\n}\r\n\r\n.contact-form input:focus,\r\n.contact-form textarea:focus {\r\n  outline: none;\r\n  background: #3a3a3a;\r\n  box-shadow: 0 0 0 2px #b266ff;\r\n}\r\n\r\n\/* Button *\/\r\n.contact-form button {\r\n  width: 100%;\r\n  padding: 15px;\r\n  background: #b266ff;\r\n  color: #fff;\r\n  font-size: 1rem;\r\n  border: none;\r\n  border-radius: 10px;\r\n  cursor: pointer;\r\n  transition: 0.3s;\r\n}\r\n\r\n.contact-form button:hover {\r\n  background: #9933ff;\r\n}\r\n\r\n\/* Animations *\/\r\n@keyframes fadeIn {\r\n  from { opacity: 0; }\r\n  to { opacity: 1; }\r\n}\r\n\r\n@keyframes slideUp {\r\n  from {\r\n    transform: translateY(50px) scale(0.95);\r\n    opacity: 0;\r\n  }\r\n  to {\r\n    transform: translateY(0) scale(1);\r\n    opacity: 1;\r\n  }\r\n}\r\n\r\n\/* Responsive *\/\r\n@media (max-width: 600px) {\r\n  .contact-container {\r\n    padding: 30px 20px;\r\n  }\r\n}\r\n"},"elements":[],"isInner":false,"widgetType":"html","elType":"widget"}],"isInner":false,"elType":"container"},{"id":"32959cee","settings":{"flex_direction":"row"},"elements":[],"isInner":false,"elType":"container"}],"page_settings":{"background_background":"classic","background_color":"#11071F"},"version":"0.4","title":"Portfolio","type":"page"}